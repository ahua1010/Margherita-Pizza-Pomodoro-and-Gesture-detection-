/*全局文字樣式設定*/
QWidget{
	color: rgb(22, 22, 22);
	font:  &quot;Segoe UI&quot;;
}

/* 應用背景 */
#bgApp {	
	background-color: rgb(247, 247, 241);
	border: 1px solid rgb(247, 247, 241);
}

/* 左側菜單 */
#leftMenuBg {	
	background-color: rgb(95, 116, 163);
}
#topLogo {
	image:url(:/image/img/tomato.png);
	background-position: centered;
	background-repeat: no-repeat;
}
#topLogoInfo
{
	background-color: rgb(95, 116, 163)
}
#titleLeftApp { font: 63 12pt &quot;Segoe UI Semibold&quot;; }
#titleLeftDescription { font: 8pt &quot;Segoe UI&quot;; color: rgb(189, 147, 249); }

/* 主菜單 */
#topMenu .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 22px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#topMenu .QPushButton:hover {
	background-color: rgb(86, 99, 134);
}
#topMenu .QPushButton:pressed {	
	background-color: rgb(189, 147, 249);
	color: rgb(255, 255, 255);
}
#bottomMenu .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color:transparent;
	text-align: left;
	padding-left: 44px;
}
#bottomMenu .QPushButton:hover {
	background-color: rgb(86, 99, 134);
}
#bottomMenu .QPushButton:pressed {	
	background-color: rgb(191, 157, 249);
	color: rgb(255, 255, 255);
}
#leftMenuFrame{
	border-top: 3px solid rgb(189, 147, 249);
}

/* 切換按鈕 */
#toggleButton {
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color: rgb(90, 108, 150);
	text-align: left;
	padding-left: 44px;
	color: rgb(113, 126, 149);
}
#toggleButton:hover {
	background-color: rgb(86, 99, 134);
}
#toggleButton:pressed {
	background-color: rgb(191, 157, 249);
}

/* 標題菜單 */
#titleRightInfo { 
	padding-left: 10px; 
	color:rgb(249, 255, 255);
	font: 10pt &quot;Segoe UI&quot;;
}

/* 擴增導航欄 */
#extraLeftBox {	
	background-color: rgb(44, 49, 58);
}
#extraTopBg{	
	background-color: rgb(189, 147, 249)
}

/* Icon */
#extraIcon {
	background-position: center;
	background-repeat: no-repeat;
	background-image: url(:/image/img/icon_settings.png);
}

/* Label */
#extraLabel { color: rgb(255, 255, 255); }

/* Btn Close */
#extraCloseColumnBtn { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#extraCloseColumnBtn:hover { background-color: rgb(196, 161, 249); border-style: solid; border-radius: 4px; }
#extraCloseColumnBtn:pressed { background-color: rgb(180, 141, 238); border-style: solid; border-radius: 4px; }

/* Extra Content */
#extraContent{
	border-top: 3px solid rgb(40, 44, 52);
}

/* Extra Top Menus */
#extraTopMenu .QPushButton {
background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 22px solid transparent;
	background-color:transparent;
	text-align: left;
	padding-left: 44px;
}
#extraTopMenu .QPushButton:hover {
	background-color: rgb(40, 44, 52);
}
#extraTopMenu .QPushButton:pressed {	
	background-color: rgb(189, 147, 249);
	color: rgb(255, 255, 255);
}

/*///////////////////////////////////////////////////////////////////

								右側內容 

////////////////////////////////////////////////////////////////////*/
#contentTopBg{	
	background-color: rgb(96, 116, 166);
}
#contentBottom{
	border-top: 3px solid  rgb(189, 147, 249);
}
/* 蕃茄鐘 */
#tomatoClockBg
{
	background-color: rgba(149, 206, 217,0.7);
	border:0px solid red;
	border-radius:30px
}
#hunkbackDetectBg
{
	background-color: rgba(255, 212, 96,0.7);
	border:0px solid red;
	border-radius:30px
}
#todolistBg
{
	background-color: rgba(183, 172, 183,0.9);
	border:0px solid red;
	border-radius:30px
}
 #todo_list{ background-color: rgba(183, 172, 183,0); }
#tomatoBg
{
	border-radius: 10px;
	border-top-right-radius: 3px;
	border-bottom-right-radius: 0px;
	background: rgba(238, 238 ,238 ,0.8);
	padding: 0px 10px 0px 5px;
}
#tomato_time{ font:  54pt;}
#tomato_mode, #tomato_bigrest{font:  14pt, &quot;Microsoft YaHei UI&quot;;}
#tomato_start{
	font:  32pt;
	border: none;
	image:url(:image/img/play.png);
}
#tomato_reset{
	font:  32pt;
	border: none;
	image:url(:image/img/reset.png);
}
#tomato_skip{
	font:  32pt;
	border: none;
	image:url(:image/img/skip.png);
}

/* Top Buttons */
#rightButtons .QPushButton { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#rightButtons .QPushButton:hover { background-color: rgb(44, 49, 57); border-style: solid; border-radius: 4px; }
#rightButtons .QPushButton:pressed { background-color: rgb(23, 26, 30); border-style: solid; border-radius: 4px; }

/* Theme Settings */
#extraRightBox { background-color: rgb(44, 49, 58); }
#themeSettingsTopDetail { background-color: rgb(189, 147, 249); }

/* Bottom Bar */
#bottomBar { background-color: rgb(73, 83, 117); }
#bottomBar QLabel { font-size: 11px; color: rgb(247, 247, 241); padding-left: 10px; padding-right: 10px; padding-bottom: 2px; }

